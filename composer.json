{
    "name": "amandiobm/rustici-engine-api-v2-client-php",
    "description": "Rustici Engine API V2 Client (PHP)",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "rustici",
        "engine"
    ],
    "homepage": "https://rustici-docs.s3.amazonaws.com/engine/2019.1.x/api/apiV2.html",
    "license": "proprietary",
    "authors": [
        {
            "name": "Rustici Software",
            "homepage": "https://rusticisoftware.com/"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "RusticiSoftware\\Engine\\V2\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "RusticiSoftware\\Engine\\V2\\" : "test/" }
    }
}
